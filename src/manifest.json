{
    "manifest_version": 2,
    "name": "Funda notities",
    "version": "1.1",

    "description": "Voeg notities to aan Funda-huizen",

    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-96.png"
    },

    "content_scripts": [
        {
            "matches": ["*://*.funda.nl/koop/*"],
            "js": ["index.js"],
            "css": ["style.css"]
        }
    ],

    "browser_specific_settings": {
        "gecko": {
            "id": "funda_notes@maybird.nl",
            "strict_min_version": "48.0"
        }
    }
}
